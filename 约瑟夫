#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i,k,m,n,num[50],a,*p;
    scanf("%d%d",&n,&a);
    p=num;
    for(i=0;i<n;i++)
    *(p+i)=i+1;
    i=0;
    k=0;
    m=0;
    while(m<n-1)
    {
                if(*(p+i)!=0)
                k++;
                if(k==a)
                {
                        printf("%d ",*(p+i));
                        *(p+i)=0;
                        k=0;
                        m++;
                }
                 i++;
                 if(i==n)
                 i=0;
    }
                 while(*p==0)
                 p++;
                 printf("%d",*p);
                 system("pause");
                 return 0;
}       
