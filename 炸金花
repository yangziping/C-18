#include<stdio.h>
#include<stdlib.h>  
int compInc(const void *a, const void *b)  
{  
    return *(int *)a - *(int *)b;  
}  
int main(){
	int a[3];
	int a1,l,q;
	for(a1=0;a1<3;a1++){
		scanf("%d",&a[a1]);
	}
	qsort(a,3,sizeof(a[0]),compInc);
	if(a[1]==a[2]&&a[0]==a[1]){
		printf("%d",10-a[1]);
	}
	else if((a[0]==a[1]&&a[0]!=a[2])||(a[2]==a[1]&&a[1]!=a[0])){
			if(a[1]!=a[0]){
				printf("%d",(9-a[1])*8+16+9-a[0]-1);
			}
			if(a[1]!=a[2]){
				printf("%d",(9-a[1])*8+16+9-a[2]);
			}
	}
	else if((a[2]-a[1]==1)&&(a[1]-a[0]==1)){
			printf("%d",9-a[2]+9);
	}
	else if(a[0]==2&&a[1]==3&&a[2]==5){
		printf("%d",165-9-4-1);
	}
	else{
		l=88;
		for(q=a[2]+1;q<=9;q++)
			l+=(q-2)*(q-3)/2+q-3;
		for(q=a[1]+1;q<a[2]-1;q++)
			l+=q-1;
		if(a[2]-a[1]!=1)
		{
			l+=a[2]-3;
			l+=a[1]-a[0]-1;
		}
		else
			l+=a[1]-a[0]-2;
					
		printf("%d",l);		
	}
	return 0;
}
