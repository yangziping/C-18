 #include<stdio.h>
#define N 10
int main()
{
  int m;//，报到m退出；
  scanf("%d",&m); 
  int num[N+1];//10个人 
  int i,j,k=0,q=0;                                                                                                                                                                                                                                                                                                               
  num[0]=1;
  for(i=1;i<=N;i++)
   num[i]=i;
  for(j=1;j<=20;j++)
  { 
    for(i=1;i<=N;)
    {
     if(num[i]!=0)
	   k++;	
     if(num[i]!=0)
      if(k%m==0)
      {
	   num[i]=0;
	   q=q+1;
      } 
       i=i+1;
	    
    }
     if(q==N-1)break;
	} 
    for(i=1;i<=N;i++)
	{ 
     if(num[i]!=0)
     printf("xuhao=%d\n",num[i]);
   }
   return 0;
}
  
