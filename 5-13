#include<iostream>
using namespace std;

struct Date{
	int year;
	int month;
	int day;
}a;

void input_f()//输入函数 
{
	cin>>a.year>>a.month>>a.day;
}

int output_f() //输出函数 
{
	cout<<a.year<<a.month<<a.day<<endl;
}

int init_f()//初始化 
{
	a.year=2018;
	a.month=4;
	a.day=1;
	cout<<a.year<<a.month<<a.day<<endl;
}

int main()
{
	input_f();
	output_f();
	init_f();
	return 0;
}
