#include<stdio.h>
#include<stdlib.h>
int main()
{
    int y(int n);
    int a,b,i,m;
    scanf("%d",&m);
    for(i=0;i<=m/2-2;i++)
    {
                         if(y(2+i)==1&&y(m-2-i)==1)
                         {
                                                   a=2+i;
                                                   b=m-2-i;
                         }
    }
    printf("%d %d",a,b);
    system("pause");
    return 0;
}

int y(int n)
{
    int i,j;
    for(i=2;i<=n/2;i++)
    if(n%i==0)
    break;
    if(i>n/2)
    j=1;
    else
    j=0;
    return j;
}
                                                   
