 ############################################################################################################################
 #include<stdio.h>
 #include<stdlib.h>
 struct Fushu
 {
 	double a;
 	double b;
 };
 void fushuadd(struct Fushu fushu1,struct Fushu fushu2);
 void fushusub(struct Fushu fushu1,struct Fushu fushu2);
 void fushumult(struct Fushu fushu1,struct Fushu fushu2);
void fushudivid(struct Fushu fushu1,struct Fushu fushu2);
 int main()
 {  
    
 	struct Fushu fushu1,fushu2;
	scanf("%lf%lf",&fushu1.a,&fushu1.b);
 	scanf("%lf%lf",&fushu2.a,&fushu2.b); 
    fushuadd(fushu1,fushu2);
    fushusub( fushu1,fushu2);
    fushumult( fushu1,fushu2);
    fushudivid(fushu1,fushu2);
   return 0;
 	
 }
void fushuadd(struct Fushu fushu1,struct Fushu fushu2)
{//相加 
    double c,d;
	c=fushu1.a+fushu2.a;
	d=fushu1.b+fushu2.b;
	printf("z=%.2lf+%.2lfi",c,d);
	printf("\n");	
 } 
void fushusub(struct Fushu fushu1,struct Fushu fushu2)
{//相减
 double c,d;
 c=fushu1.a-fushu2.a;
 d=fushu1.b-fushu2.b;
 printf("z=%.2lf+%.2lfi",c,d);
 printf("\n");	
 } 
void fushumult(struct Fushu fushu1,struct Fushu fushu2)
{
	//相乘 
	double c,d;
	c=fushu1.a*fushu2.a-fushu1.b*fushu2.b;
	d=fushu1.b*fushu2.a+fushu1.a*fushu2.b;
	printf("z=%.2lf+%.2lfi",c,d);
	printf("\n");
 } 
void fushudivid(struct Fushu fushu1,struct Fushu fushu2)
{   double c,d;
	c=(fushu1.a*fushu2.a+fushu1.b*fushu2.b)/(fushu2.a*fushu2.a+fushu2.b*fushu2.b);
	d=(fushu1.b*fushu2.a-fushu1.a*fushu2.b)/(fushu2.a*fushu2.a+fushu2.b*fushu2.b);
	printf("z=%.2lf+%.2lfi",c,d);
	printf("\n");
 } 
 
